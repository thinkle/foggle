{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Vitest",
			"program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
			"args": [
				"--run",
				"--inspect-brk",
				"--no-file-parallelism",
				"src/lib/wordFeedback.test.ts"
			],
			"autoAttachChildProcesses": true,
			"console": "integratedTerminal",
			"skipFiles": ["<node_internals>/**", "**/node_modules/**"]
		}
	]
}
